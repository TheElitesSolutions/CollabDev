# =================================
# CollabDev+ Root .gitignore
# =================================

# Dependencies
node_modules/

# Build outputs
dist/
build/
.next/
out/

# Logs
logs/
*.log
npm-debug.log*
pnpm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# OS files
.DS_Store
Thumbs.db
*.swp
*.swo

# IDE
.idea/
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
*.sublime-workspace
*.sublime-project

# Environment files
.env
.env.local
.env.*.local
.env.production
.env.staging
!.env.example
!.env.*.example

# Docker
.docker/
docker-compose.override.yml

# Coverage
coverage/
.nyc_output/

# Cache
.turbo/
.cache/
*.tsbuildinfo

# Temp
.temp/
.tmp/
tmp/

# Runtime
pids/
*.pid
*.seed
*.pid.lock

# SSL certificates (keep examples)
*.pem
*.key
*.crt
!*.example.pem
!*.example.key
!*.example.crt

# Database files
*.sqlite
*.sqlite3
*.db

# Backup files
*.bak
*.backup
